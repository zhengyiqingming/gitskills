<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.dao.ChapterDao">
	
	<!-- 添加章节 -->
	<insert id="insertItem" parameterType="com.baizhi.entity.Chapter">
 		insert into chapter (title,size,duration,downloadPath,uploadDate,albumId) 
 					values(#{title},#{size},#{duration},#{downloadPath},#{uploadDate},#{albumId})
 	</insert>
	
</mapper>
